<!-- website pagination -->
<nav aria-label="Page navigation" class="paginator">
    <ul class="pager">
        <% if (page.prev) { %>
          <li><a href="<%- config.root %><%- page.prev_link %>"><%- __('prev')%></a></li>
        <% } else { %>
          <!-- <li class="disabled"><a href="javascript:void(0);"><%- __('prev') %></a></li> -->
          <li class="disabled"><a href="#" onclick="javascript:return false;"><%- __('prev') %></a></li>
        <% } %>

        <% if (page.next) { %>
          <li><a href="<%- config.root %><%- page.next_link %>"><%- __('next')%></a></li>
        <% } else { %>
          <li class="disabled"><a href="#" onclick="javascript:return false;"><%- __('next') %></a></li>
        <% } %>
    </ul>
</nav>
